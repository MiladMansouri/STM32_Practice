#ifndef DEVICEIO_H_
#define DEVICEIO_H_
#include "DeviceIO.h"

class DeviceIO
{
public:
	DeviceIO();
	virtual ~DeviceIO();
	DeviceIO(const DeviceIO &other);
	DeviceIO(DeviceIO &&other);
	DeviceIO& operator=(const DeviceIO &other);
	DeviceIO& operator=(DeviceIO &&other);
	bool write(uint8_t data);
};

#endif /* DEVICEIO_H_ */
